{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "amproject1adf"
		},
		"ADLS_proj1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_proj1'"
		},
		"SQL_DB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_DB_LS'"
		},
		"AzureSqlDatabase_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_LS'"
		},
		"ADLS_proj1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://amproject1.dfs.core.windows.net/"
		},
		"AmazonS3_LS_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault1",
					"type": "LinkedServiceReference"
				},
				"secretName": "s3accesskeyid"
			}
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://amproject1keyvault.vault.azure.net/"
		},
		"SQL_DB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "amproject1dbserver1.database.windows.net"
		},
		"SQL_DB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azuredbproject1"
		},
		"SQL_DB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Amsqladmin"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "amproject1dbserver1.database.windows.net"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "proj1azuresqldb"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Amsqladmin"
		},
		"Storage_Trigger_For_JSON_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1709eb87-a39f-427e-aa29-14121327f7a4/resourceGroups/Project1/providers/Microsoft.Storage/storageAccounts/amproject1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "amproject1adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_proj1_accountKey": {
						"value": "[parameters('ADLS_proj1_accountKey')]"
					},
					"SQL_DB_LS_password": {
						"value": "[parameters('SQL_DB_LS_password')]"
					},
					"AzureSqlDatabase_LS_password": {
						"value": "[parameters('AzureSqlDatabase_LS_password')]"
					},
					"ADLS_proj1_properties_typeProperties_url": {
						"value": "[parameters('ADLS_proj1_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"SQL_DB_LS_properties_typeProperties_server": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_server')]"
					},
					"SQL_DB_LS_properties_typeProperties_database": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_database')]"
					},
					"SQL_DB_LS_properties_typeProperties_userName": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_userName')]"
					},
					"AmazonS3_LS_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS3_LS_properties_typeProperties_accessKeyId')]"
					},
					"Storage_Trigger_For_JSON_properties_typeProperties_scope": {
						"value": "[parameters('Storage_Trigger_For_JSON_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}